	technique = {Topology},
	title = {Robust topological methods for analysis of dynamic large scale data for modern material design},
	year = {2019},
	Bdsk-File-1 = {}}

@proposal{Hotz2019-Prop,
	abstract = {In this project we plan to perform research on topological data analysis with the goal to enable robust, multi-scale feature-tracking targeting guidance for the visual exploration of large-scale data sets to gain new insights into the dynamics of the data. The project is driven by scientific questions from selected applications in engineering, physics and medicine with increasing data complexity challenges. Our approach is to define descriptive data summaries on multiple scales, extending the idea of barcodes, which are given as feature vectors. We will establish feature evolution graphs through parameter spaces or time to highlight structural changes. The graphs will serve as the backbone of an interactive comparison and parameter space exploration. Starting with scalar fields the methods will be extended to less explored data types such as tensor fields and distributions . The development of robust and efficient algorithms will be a central theme. To reach the ambitious goals several fundamental questions have to be solved including the definition and investigation of appropriate interpolation methods and metrics in the space of tensors and distributions. The results will be implemented in a visual data analysis and exploration environment for scientific reasoning and interaction with the data.},

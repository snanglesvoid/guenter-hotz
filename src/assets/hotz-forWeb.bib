@article{Yan2020-R,
	abstract = { Merge trees, a type of topological descriptors for scalar fields, serve to identify and summarize the topological characteristics associated with a data instance. They present a great potential for the analysis and visualization of ensembles, that is, collections of data instances that arise from multiple simulation runs, different parameter settings, or time-varying observations. First, they provide compressed and topology-preserving representations of ensemble members. Second, their comparison provides a basis for studying the relations among ensemble members, such as their distributions, averages, clusters, outliers, and periodicities. Some comparative metrics have been developed for merge trees including functional distortion distance, Gromov--Hausdorff distance, and edit distance. However, these metrics are often computationally expensive since they implicitly consider all possible correspondences between critical points of the merge trees without integrating geometric information from the data domain. In this paper, we perform geometry and topology aware comparisons of merge trees. The main idea is to decouple the computation of a comparative metric into two steps: a labeling step that generates a correspondence between the critical points of two merge trees, and a distance computation step that computes distances between a pair of labeled merge trees by encoding them as matrices. We show that our approach is both computationally efficient and practically useful. Our general framework makes it possible to integrate both geometric and topological information of the data domain in the labeling process. At the same time, it reduces the computational complexity significantly since not all possible correspondences have to be considered. We provide several heuristic strategies for the labeling of critical points in a merge tree. We also investigate various theoretical properties of distance metrics that may be desirable to merge tree metrics. We further provide an interactive visualization system that demonstrates the utility of our framework across a set of scalar field ensembles.},
	application = {NA},
	author = {Lin Yan and Talha Bin Masood and Ingrid Hotz and Bei Wang},
	funding = {NA},
	journal = {{IEEE Transactions on Visualization and Computer Graphics (Vis - rejected}},
	keypicture = {./key_pictures/NA},
	keywords = {merge trees, topology, signature, scalar field, comparison, metric},
	pdf = {./pdf/NA},
	technique = {NA},
	title = {Geometry and Topology Aware Merge Tree Metrics for Ensembles},
	year = {2020 - rejected},

@inproceedings{Engelke2020,
	abstract = {In this paper, we propose a concept to design, track and compare application- specific topology-based features as a set of critical points. Our work has been inspired by the observation that in many applications a large variety of feature definitions are used. Furthermore, it is often unclear which definition should be used in which con- text. Often, these features can be related to topological concepts, including extremal structures. Given this, we focus on features spanned by sets of critical points. In our approach, we separate the topological analysis and the critical point tracking from the feature design and thus facilitate a means for interactive feature comparison. The backbone of our tracking method is a precomputed tracking graph which stores all extremal points as well as their temporal corespondents. In addition, features are specified by rules for grouping critical points and represented by a respective area in the geometric embedding. Now, feature tracking corresponds to filtering and querying the full tracking graph by specific requests. We demonstrate this method for cyclone identification and tracking in the context of climate research.},
	annote = {http://arxiv.org/abs/2011.08676},
	application = {Meteorology},
	author = {Wito Engelke and Jakob Beran and Rodrigo Caballero and Talha Bin Masood and Ingrid Hotz},
	booktitle = {Topologicla Methods in Data Analysis and Visualization},

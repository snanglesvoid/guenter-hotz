	keypicture = {./key_pictures/NA},
	keywords = {in situ, topolgy, flow},
	pdf = {./pdf/NA},
	technique = {Topology, Big Data},
	title = {In-Situ Big Data Analysis for Flow and Climate Simulations},
	year = {2016},
	Bdsk-File-1 = {},
	Bdsk-File-2 = {}}

@proposal{Hotz2019B-Prop,
	abstract = {Large scale imaging and simulation is a common way to approach scientific problems. The consequence is the generation of data with increasing size and complexity with a need for robust and efficient data analysis methods to aid the domain experts in the scientific discovery process. However, many such techniques are developed in an ad hoc and heuristic manner. Techniques from computational topology provide robust methods for data analysis. However, to be useful in real world applications it is needed to link these concepts to semantic feature definitions in the application domain so that experts can make meaningful interpretations from the abstract representations. In this project, we will develop a pipeline for topological feature extraction, comparison, and tracking in two focus application areas relevant for the development of materials of the future. The first involves dynamic data from imaging in material research and the second is the study of data from molecular dynamics simulation of proteins. Both data sources are challenging in terms of complexity and size. Starting with an appropriate topological representation of the data we will design methods for studying similarities and differences between data sets. Next, we will develop algorithms for extracting and tracking salient features over time, highlighting dynamical changes in the data. All developed methods will be integrated into an interactive visualization tool targeted at domain scientists.},
	application = {Material science},
	author = {Ingrid Hotz and Vijay Natarajan},
	call = {Indo Swedish joint network grant},
	collaborators = {Anders Ynnerman and Mathieu Linares and Talha Bin Masood and Tejas Gorur Murthy},
	funding = {VR},
	keypicture = {./key_pictures/NA},
	keywords = {Topology},

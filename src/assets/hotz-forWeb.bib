@article{Nielsson2021,
	abstract = {We present a framework for tracking and visualizing the evolution of cyclones which we organize in hierarchical groups. Our approach is based on the general concept of critical point tracking and supports the exploration of resulting tracks in a level of detail approach. The problem of tracking and visualizing cyclones is still an active area of climate research, since the nature of cyclones varies depending on geospatial location and temporal season, resulting in no clear mathematical definition. Consequently, many methods contain multiple parameters used to distinguish cyclones of interest but do not work for general cyclone extraction across the globe. Our pipeline consists of four modular building blocks: (1) a novel critical point tracking method, used in a time-varying scalar field, (2) defining cyclonic features as groups of critical points based on merge-tree crown components, (3) tracking features and creating the hierarchy between them and (4) a hierarchical visualization of features containing a global abstract representation linked to a spatial embedding, allowing exploration on a global and local scale. The modularity of the pipeline allows each independent block to be exchanged with another method conforming to expected input and output.  We demonstrate that our method is not limited to the specifics of cyclone tracking and can be used in other application areas.},
	application = {Meteorology},
	author = {Emma Nilsson and Jonas Lukasczyk and Wito Engelke and Talha Bin Masood and Rodrigo Caballero and Gunilla Svensson and Christoph Garth and Ingrid Hotz},
	funding = {SeRC; SSF},
	journal = {IEEE Transactions on Visualization and Computer Graphics (TVCG)},
	keypicture = {./key_pictures/NA},
	pdf = {./pdf/NA},
	technique = {Topology},
	title = {Understanding Cyclone Evolution: A modular framework for hierarchical feature tracking},
	year = {2021 submitted}}

@article{Thygesen2021,
	abstract = {We present a framework for the exploration and analysis of ensembles of transition diagrams, where a transition diagram describes the change of distribution between two different states.  Specifically we are interested in electronic transitions in a molecule. Such transitions can be divided into local excitation - the electron density field is similar before and after the transition, and charge transfer - the electron density distribution shifts from one part to another part of the molecule during the electronic transition. It is of high interest to distinguish between these types of transitions when developing a better understanding of the interaction between materials and light on a molecular level.One challenge is to compare such electronic transitions, both between different excited states of the same molecule (for one or several conformations) but also between different molecule ensembles. In this paper, we propose a way to explore an ensemble of electronic transitions by representing the transition with a feature vector and applying dimensionality reduction and hierarchical clustering to group similar transitions together. We introduce a mean transition diagram to represent a cluster of transitions, shown at different level of detail depending on available space. The implementation is done in an interactive tool, allowing the user to explore similar transitions at different levels of the cluster hierarchy.},
	application = {Electronic structures; Molecular visualization; theoretical Chemistry},
	author = {Signe Sidwall Thygesen and Talha Bin Masood and Mathieu Linares and Ingrid Hotz},
	funding = {SeRC, VR},
	journal = {{IEEE Transactions on Visualization and Computer Graphics (TVCG)}},
	keypicture = {./key_pictures/NA},
	pdf = {./pdf/NA},
	technique = {Ensemble visualizaiton},
	title = {Level of detail exploration of transition diagram ensembles},
	year = {2021 submitted}}

@article{BinMasood2021c,
	abstract = {The study of electronic transitions within a molecule connected to the absorption or emission of light is a common task in the process of the design of new materials. The transitions are complex quantum mechanical processes and a detailed analysis requires a breakdown of these processes into components that can be interpreted via characteristic chemical properties. We approach these tasks by providing a detailed analysis of the electron density field. This entails methods to quantify and visualize electron localization and transfer from molecular subgroups combining spatial and abstract representations. The core of our method uses geometric segmentation of the electronic density field coupled with a graph theoretic formulation of charge transfer between molecular subgroups. The design of the methods has been guided by the goal of providing a generic and objective analysis following fundamental concepts. We illustrate the proposed approach using several case studies involving the study of electronic transitions in different molecular systems.},
	application = {Electronic stuctures; Molecular visualization; theoretical Chemistry},
	author = {Talha Bin Masood and Signe Sidwall Thygesen and Mathieu Linares and Alexei Abrikossov and Vijay Natarajan and Ingrid Hotz},
	funding = {SeRC, VR, IndoSwedish},
	journal = {{Computer Graphics Forum}},
	keypicture = {./key_pictures/binMasood21_chageTransistion.pdf},
	keywords = {charge density, charge transition, molecular visualization, chemistry},
	pdf = {./pdf/binMasood21_chageTransistion.pdf; ./pdf/binMasood21_chageTransistion_sup.pdf},
	read = {1},
	technique = {Segmentation, Topology},
	title = {Visual Analysis of Electronic Densities and Transitions in Molecules},
	volume = {xx},
	year = {2021}}


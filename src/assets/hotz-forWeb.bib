	funding = {NA},
	journal = {Computer Graphics Forum (EuroVis'19)},
	keypicture = {./key_pictures/NA},
	keywords = {robustness, topology, tensor fieldw, inding-number, degree of a function, index theory, Poincare-Hopf Theorem, well group, critical points, degenerate points, eigenvector field, bidirectional anisotropy vectors, anisotropy vector field, projective line},
	number = {3},

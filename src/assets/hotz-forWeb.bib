@article{Engelke2018b,
	abstract = {We present an interactive approach to analyze flow fields using a new type of particle system, which is composed of autonomous particles exploring the flow. While particles provide a very intuitive way to visualize flows, it is a challenge to capture the important features with such systems. Particles tend to cluster in regions of low velocity and regions of interest are often sparsely populated. To overcome these disadvantages we propose an automatic adaption of the particle density with respect to local importance measures, which can be selected and the systems sensitivity to them interactively changed by the user. Together with the particle history these measures define a probability for particles to multiply or die respectively. There is no communication between the particles and no neighborhood information has to be maintained. Thus, the particles can be handled in parallel and support a real-time investigation of flow fields. To enhance the visualization, the particles properties and selected field measures are also used to specify the systems rendering parameters, as color and size. We demonstrate the effectiveness of our approach on different, simulated vector fields from technical and medical applications.},
	application = {NA},
	author = {Wito Engelke and Ingrid Hotz},
	date-added = {2018-03-13 16:14:26 +0000},
